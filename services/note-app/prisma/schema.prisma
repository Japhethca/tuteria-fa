generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alembic_version {
  version_num String @id
}

model notes {
  id          Int     @default(autoincrement()) @id
  title       String?
  description String?
  user        String?
}

model users {
  id       Int     @default(autoincrement()) @id
  username String? @unique
  password String?
  email    String? @unique
  role     String?
}
