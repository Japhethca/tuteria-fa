generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alembic_version {
  version_num String @id
}

model notes {
  id          Int     @default(autoincrement()) @id
  title       String?
  description String?
  owner       String?
}

model users {
  id       Int     @default(autoincrement()) @id
  name     String?
  username String?
}

model admins {
  id   Int     @default(autoincrement()) @id
  key  String?
  hash String?
}
